<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>hello</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>form</servlet-name>
        <servlet-class>form</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>compute</servlet-name>
        <servlet-class>compute</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PassParams</servlet-name>
        <servlet-class>PassParams</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>author</servlet-name>
        <servlet-class>author</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserInfo</servlet-name>
        <servlet-class>UserInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/User</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>form</servlet-name>
        <url-pattern>/form</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>compute</servlet-name>
        <url-pattern>/compute</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PassParams</servlet-name>
        <url-pattern>/PassParams</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>author</servlet-name>
        <url-pattern>/author</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserInfo</servlet-name>
        <url-pattern>/UserInfo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
